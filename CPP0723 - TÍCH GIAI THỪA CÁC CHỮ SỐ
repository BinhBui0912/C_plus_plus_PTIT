#include<bits/stdc++.h>

using namespace std;

int main(){
	int t;
	cin >> t;
	while(t--){
		int n;
		string a;
		cin >> n;
		cin >> a;
		string cnt = "";
		for(int i = 0; i < n; i++)
		{
			switch(a[i])
			{
				case '2':
					{
						cnt += "2";
						break;	
					}
				case '3':
					{
						cnt += "3";
						break;
					}
				case '4':
					{
						cnt += "322";
						break;
					}
				case '5':
					{
						cnt += "5";
						break;
					}
				case '6':
					{
						cnt += "53";
						break;
					}
				case '7':
					{
						cnt += "7";
						break;
					}
				case '8':
					{
						cnt += "7222";
						break;
					}
				case '9':
					{
						cnt += "7332";
						break;
					}
				default:
					{
						break;
					}
			}
		}
		sort(cnt.begin(), cnt.end());
		for(int i = cnt.size() - 1; i >= 0; i--) cout << cnt[i] ; 
		cout << endl;
	}
}
